{
  "name": "stackview",
  "displayName": "StackView",
  "description": "Navigate code definitions in a stacked view",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "stackview.goToDefinition",
        "title": "Go to Definition (StackView)"
      },
      {
        "command": "stackview.clearStack",
        "title": "StackView: Clear Stack"
      }
    ],
    "keybindings": [
      {
        "command": "stackview.goToDefinition",
        "key": "alt+cmd+f12",
        "mac": "alt+cmd+f12",
        "win": "alt+ctrl+f12",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "stackview.goToDefinition",
          "when": "editorTextFocus",
          "group": "navigation@1.5"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}