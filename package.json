{
  "name": "stackview",
  "displayName": "StackView",
  "description": "Navigate code definitions in a stacked view instead of opening multiple tabs",
  "version": "0.0.1",
  "publisher": "AliciaSun",
  "author": {
    "name": "Alicia Sun"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Alicia-Sun/stackview.git"
  },
  "bugs": {
    "url": "https://github.com/Alicia-Sun/stackview/issues"
  },
  "homepage": "https://github.com/Alicia-Sun/stackview#readme",
  "keywords": [
    "debug",
    "definition",
    "stack",
    "code",
    "goto"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": ["Other"],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "stackview.goToDefinition",
        "title": "Go to Definition (StackView)"
      },
      {
        "command": "stackview.clearStack",
        "title": "StackView: Clear Stack"
      }
    ],
    "keybindings": [
      {
        "command": "stackview.goToDefinition",
        "key": "alt+cmd+f12",
        "mac": "alt+cmd+f12",
        "win": "alt+ctrl+f12",
        "when": "editorTextFocus"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "stackview.goToDefinition",
          "when": "editorTextFocus",
          "group": "navigation@1.5"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/vscode": "^1.74.0",
    "@types/node": "16.x",
    "typescript": "^4.9.4"
  }
}